{
  "Version": "2012-10-17",
  "Id": "arn:aws:sqs:us-west-2:801351377084:thor-20180703-lsdc-builtin/SQSDefaultPolicy",
  "Statement": [
    {
      "Sid": "allow-sqs-publish-from-s3",
      "Effect": "Allow",
      "Principal": {
        "Service": "s3.amazonaws.com"
      },
      "Action": [
        "sqs:Get*",
        "sqs:List*",
        "sqs:Send*"
      ],
      "Resource": "arn:aws:sqs:us-west-2:801351377084:thor-20180703-lsdc-builtin",
      "Condition": {
        "ArnLike": {
          "aws:SourceArn": "arn:aws:s3:::thor-20180703"
        }
      }
    }
  ]
}
